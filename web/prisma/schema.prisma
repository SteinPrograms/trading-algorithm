generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model logs {
  id            Int      @id @default(autoincrement())
  log_level     Float?   @db.Real
  log_levelname String   @db.VarChar(255)
  log_content   String   @db.VarChar(2048)
  created_at    DateTime @db.Timestamp(6)
}

model positions {
  id            Int       @id @default(autoincrement())
  open_price    Float?    @db.Real
  close_price   Float?    @db.Real
  highest_price Float?    @db.Real
  lowest_price  Float?    @db.Real
  current_price Float?    @db.Real
  open_date     DateTime? @db.Timestamp(6)
  close_date    DateTime? @db.Timestamp(6)
  status        String?   @db.VarChar(255)
  exit_mode     String?   @db.VarChar(255)
}

model vitals {
  id               Int     @id @default(autoincrement())
  status           String? @db.VarChar(255)
  total_net_return Float?  @db.Real
  running_time     String? @db.VarChar(255)
  current_price    Float?  @db.Real
}
